        stroke="black"
        stroke-width="2"
      />
      <line
        x1="4.22"
        y1="19.78"
        x2="6.34"
        y2="17.66"
        stroke="black"
        stroke-width="2"
      />
      <line
        x1="17.66"
        y1="6.34"
        x2="19.78"
        y2="4.22"
        stroke="black"
        stroke-width="2"
      />
